    "    )\n",
    "\n",
    "# --- plot cumulative annualized vol-scaled returns ---\n",
    "plt.figure(figsize=(12, 6))\n",
    "for b in bin_w_spread:\n",
    "    cols = {f\"{b}_vol_scaled_ann_cum\", \"date\", \"0_vol_scaled_ann_cum\", \"9_vol_scaled_ann_cum\"}\n",
    "    sub = rets.select(cols).to_pandas()\n",
    "    x = sub[\"date\"]\n",
