   "cell_type": "code",
   "execution_count": 59,
   "id": "078e3c71",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 1200x600 with 1 Axes>"
