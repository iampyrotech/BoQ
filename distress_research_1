    "# Define failure codes\n",
    "failure_codes = [552, 560, 561, 572, 574, 580, 581, 582, 583, 584, 585, 586, 587, 591]\n",
    "# failure_codes = [560,574,552]\n",
    "# failure_codes=range(550,600)\n",
    "# failure_codes=[574]\n",
    "# failure_codes=[591]\n",
    "\n",
    "\n",
    "data = data.with_columns(\n",
    "    pl.col(\"DLSTCD\")\n",
    "      .is_in(failure_codes)\n",
    "      .fill_null(False)           \n",
    "      .cast(pl.Int8)              \n",
    "      .alias(\"failure\")\n",
    ")\n"
   ]
